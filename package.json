  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --aot --base-href=/steam-chat-skin-website/",
    "build:github": "ng build --prod --aot --base-href=/steam-chat-skin-website/",
    "build:gitlab": "ng build --prod --aot --base-href=/steam-skin/",
